@model LPFactory.Data.ArticuloFamilia

@{
    ViewData["Title"] = "Crear";
}

@using (Html.BeginForm())
{
    using (Html.Awe().BeginContext())
    {
        @Html.EditorFor(o => o.Codigo)
        @Html.EditorFor(o => o.Descripcion)
        @Html.EditorFor(o => o.Actividad)
        @Html.EditorFor(o => o.Activo)
    }
}